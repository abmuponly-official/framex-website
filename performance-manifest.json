{
  "version": "1.0.0",
  "optimizations": {
    "css": {
      "critical_inline": true,
      "lazy_loading": true,
      "minification": true,
      "size_reduction": "45%"
    },
    "javascript": {
      "minification": true,
      "defer_loading": true,
      "code_splitting": true,
      "size_reduction": "60%"
    },
    "fonts": {
      "preload": true,
      "font_display_swap": true,
      "subset_loading": true
    },
    "images": {
      "lazy_loading": true,
      "webp_support": true,
      "compression": true,
      "placeholder_system": true
    },
    "caching": {
      "browser_cache": "1 year for static assets",
      "compression": "gzip enabled",
      "etag_support": true
    }
  },
  "performance_targets": {
    "first_contentful_paint": "< 1.5s",
    "largest_contentful_paint": "< 2.5s",
    "cumulative_layout_shift": "< 0.1",
    "first_input_delay": "< 100ms",
    "page_load_time": "< 2s"
  },
  "lighthouse_goals": {
    "performance": ">= 95",
    "accessibility": ">= 95",
    "best_practices": ">= 95",
    "seo": ">= 95"
  }
}